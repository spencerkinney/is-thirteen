  s.homepage    = 'https://github.com/spencerkinney/is-thirteen'
  s.license     = 'MIT'
  s.required_ruby_version = '>= 2.6.0'
  
  s.metadata = {
    "source_code_uri" => "https://github.com/spencerkinney/is-thirteen",
